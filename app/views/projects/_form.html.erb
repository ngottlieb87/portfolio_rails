<%= render "layouts/errors", :object => @project %>

<%= form_for @project do |f| %>
  <div class="col-4">
    <div class="form-group">
      <%= f.label :title, :class => "form_title" %>
      <%= f.text_field :title, :class => "form-control" %>
      <%= f.label :project_link, :class => "form_title" %>
      <%= f.text_field :project_link, :class => "form-control" %>
    <span class="input-group-btn">
      <%= f.submit :class => "btn btn-primary"%>
    </span>
    </div>
  </div>
<% end %>
<p><%= link_to "Return to Projects", project_path(@project.id)%></p>
