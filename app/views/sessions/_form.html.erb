<div class = "card">
  <div class= "card-body">
  <h1>Sign in</h1>
  <%= form_tag signin_path do %>
    <%= label_tag :email %>
    <%= text_field_tag :email,"", class:"form-control" %>
    <%= label_tag :password %>
    <%= password_field_tag :password,"", class:"form-control" %>
    <%= submit_tag "Sign in", class:"btn btn-success form-control", id:"sign_in_button" %>
  <% end %>
  </div>
  <%= link_to "Back", projects_path, class:"back" %>
</div>
